@model Category


@{
    ViewData["Title"] = "Create";
}

<div class="container my-5">

    <form class="form-sample" asp-action="Create" >

        <input type="text" class="form-control" id="customFile" asp-for="Name"/>   @*Bu name Category nin ichinde olan name propertisidi*@
        <div class="buttons my-3">
           <button type="submit" class="btn btn-primary mr-2">Submit</button>
           <a class="btn btn-light" asp-action="Index">Cancel</a>
        </div>

    </form>
</div>

